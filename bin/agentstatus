#!/bin/bash
############################################################################################################################
#
# Outsourcer Agent status script.
#       
############################################################################################################################
set -e  
PWD=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $PWD/../os_path.sh

# find the pid for Outsourcer and kill it
pid=$(ps -ef 2> /dev/null | grep $OSJAR | grep AgentD | grep -v grep | awk '{ print $2 }')

if [ $pid ]; then
        echo Up
else
	echo Down
fi
