#!/bin/bash
############################################################################################################################
#
# Outsourcer start all script
#
############################################################################################################################
PWD=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
source $PWD/../oracle2gp_path.sh

echo "############################################################################################################################"
echo "#"
echo "# Start UI"
echo "#"
echo "############################################################################################################################"
uistart

echo "############################################################################################################################"
echo "#"
echo "# Start Queue Daemon"
echo "#"
echo "############################################################################################################################"
osstart

echo "############################################################################################################################"
echo "#"
echo "# Start Scheduler Daemon"
echo "#"
echo "############################################################################################################################"
agentstart

